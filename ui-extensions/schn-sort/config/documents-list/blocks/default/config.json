{
    "evaluator": "or",
    "condition": [
        {
            "evaluator": "uri-match",
            "condition": "/projects/{projectId}/branches/{branchId}/documents"
        },
        {
            "evaluator": "uri-match",
            "condition": "/projects/{projectId}/branches/{branchId}/documents/{documentId}/browse"
        }
    ],
    "config": {
        "regions": {
            "center": {
                "type": "documents-list",
                "config": {
                    "replace": true,
                    "chrome": false,
                    "buttons": [],
                    "icon": true,
                    "checkbox": true,
                    "loader": "gitana",
                    "actions": true,
                    "selectorGroups": {
                        "sort-selector-group": {
                            "fields": [
                                {
                                    "key": "year",
                                    "title": "Year",
                                    "field": "year"
                                },
                                {
                                    "key": "author",
                                    "title": "Author",
                                    "field": "contributors.authors"
                                }
                            ]
                        }
                    }
                }
            }
        },
        "documents-list-buttons": [{
            "key": "new",
            "buttons": [{
                "key": "upload_csv",
                "title": "Upload Endnote CSV",
                "iconClass": "fad fa-table",
                "align": "left",
                "action": "upload-endnote-csv",
                "allowPermission": [
                    "create_subobjects"
                ]
            }]
        }]
    }
}